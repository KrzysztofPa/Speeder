{
  "manifest_version": 2,
  "name": "Speeder",
  "description": "Umożliwia przyspieszanie filmów video",
  "version": "1.1",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}